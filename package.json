{
	"name": "reserved-slugs",
	"version": "1.0.1",
	"description": "A comprehensive list of 1,000+ reserved slugs and usernames to prevent URL collisions in web applications",
	"license": "MIT",
	"author": "miketromba",
	"repository": {
		"type": "git",
		"url": "https://github.com/miketromba/reserved-slugs.git"
	},
	"homepage": "https://github.com/miketromba/reserved-slugs",
	"bugs": {
		"url": "https://github.com/miketromba/reserved-slugs/issues"
	},
	"keywords": [
		"reserved",
		"slugs",
		"usernames",
		"blocklist",
		"blacklist",
		"url",
		"routing",
		"saas",
		"validation",
		"reserved-usernames",
		"reserved-words",
		"slug-validation",
		"username-validation",
		"url-slug",
		"forbidden-usernames",
		"blocked-usernames"
	],
	"sideEffects": false,
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"data",
		"README.md"
	],
	"scripts": {
		"build": "tsup && bun run scripts/generate-formats.ts",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"test": "bun test",
		"prepare": "husky"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.4.2",
		"@types/bun": "^1.3.9",
		"husky": "^9.1.7",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	}
}
